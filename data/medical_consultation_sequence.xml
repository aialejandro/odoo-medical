<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Sequence for Medical Consultation -->
    <record id="seq_medical_consultation" model="ir.sequence">
        <field name="name">Medical Consultation</field>
        <field name="code">medical.consultation</field>
        <field name="prefix">CONS</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Sequence for Medical Record Number -->
    <record id="seq_medical_record" model="ir.sequence">
        <field name="name">Medical Record</field>
        <field name="code">medical.record</field>
        <field name="prefix">HC</field>
        <field name="padding">6</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
